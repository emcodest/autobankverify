
<div class="well">

   
        <br>
        <h2>Uploaded Batches</h2>
        <br>
    
   
    <div class="row align-items-center">
        <div class="col-lg-4">
           
        </div>
        <div class="col-lg-8 col-md-12">
            <div class="row">
                <div class="col-md-4 col-lg-6 d-flex justify-content-md-end sort-index">
                   
                 
                </div>
             
            </div>
        </div>
    </div>
</div>
<div class="table-responsive" style = "border: 1px solid red">
    <table id = "dtable" class="table m-0">
        <thead>
            <tr class="bg-fade">
                <th></th>
                <th style="width: 120px;">Date</th>
                <!-- <th style="width: 120px;">Date</th> -->
               
                <th>Batch Name</th>
                <th style="width: 100px;"># Total</th>
               
                <th style="width: 100px">Report</th>
                <th>Action</th>
              
            </tr>
        </thead>
        <tbody>
            <% for(let oo of batches){ %> 

            <tr>
                <td></td>
                <td class="align-middle"><%= oo.created_at %></td>
                <td class="align-middle">
                    <div>
                        <h5><%= oo.batch_name %></h5>
                    </div>

                </td>
                <td class="align-middle">
                    <div>
                        <h5><%= oo.cnt %></h5>
                        <!-- <em class="text-muted ml-1">(Sales Manager)</em> -->
                    </div>

                </td>

                <td class="align-middle">
                  <a class = "btn btn-primary" href = "/user/report?_id=<%= oo.id %>">View Report</a>
                </td>
                <td class="align-middle">
                <button func = "DeleteBatch" params = "<%= oo.id %>"  class = "btn btn-danger click">Delete Batch</button>
                </td>
               
            </tr>
        <% } %> 

  
        </tbody>
    </table>
</div>